    <connection>erp_oltp</connection>
    <sql>SELECT
p.DATA_PEDIDO ,
p.CODIGO_CLIENTE AS COD_CLI,
p.CODIGO_VENDEDOR,
pr.CODIGO_PRODUTO as CODIGO_PRODUTO,
c.CIDADE,
c.ESTADO,
l.QUANTIDADE,
l.QUANTIDADE * pr.PRECO as VLR_VENDA
FROM
item_pedidos l
inner join pedidos p
on l.NUMERO_PEDIDOS = p.NUMERO_PEDIDOS
inner join produtos pr
on l.CODIGO_PRODUTO = pr.CODIGO_PRODUTO
inner join clientes c
on p.CODIGO_CLIENTE = c.CODIGO_CLIENTE</sql>
